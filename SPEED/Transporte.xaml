                    </Canvas>
                </Canvas>
            </Grid>
            <Button Uid="0" Content="         Camiones" HorizontalAlignment="Left" Height="42" Margin="0" VerticalAlignment="Top" Width="143" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FF343434" FontSize="14" FontFamily="Aller" BorderThickness="1,0,0,0" Click="Button_Click" Grid.ColumnSpan="2"/>
            <Button Uid="1" Content="          Rutas" HorizontalAlignment="Left" Height="42" Margin="19,0,0,0" VerticalAlignment="Top" Width="143" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FF343434" FontSize="14" FontFamily="Aller" BorderThickness="1,0,0,0" Click="Button_Click" Grid.Column="1"/>
            <Button Uid="2" Content="         Dispositivos" HorizontalAlignment="Left" Height="42" Margin="162,0,0,0" VerticalAlignment="Top" Width="143" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FF343434" FontSize="14" FontFamily="Aller" BorderThickness="1,0,0,0" Click="Button_Click" Grid.Column="1"/>
        </Grid>
        <Frame x:Name="PGrid" VerticalAlignment="Bottom" Height="608"/>
        <Rectangle x:Name="Cursor" Fill="#FFFFD200" HorizontalAlignment="Left" Height="3" VerticalAlignment="Top" Width="143"/>
    </Grid>
</UserControl>
