                </Canvas>
            </Grid>
            <Grid Width="42" Height="42" HorizontalAlignment="Left" Margin="42,0,0,0" Grid.Column="1">
                <Canvas Width="18.000" Height="20.000" Margin="0">
                    <Canvas>
                        <!-- Capa 1/<Trazado> -->
                        <Path StrokeThickness="2.0" Stroke="#FF343434" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeLineJoin="Round" Data="F1 M 1.000,5.000 L 1.000,21.000 L 8.000,17.000 L 16.000,21.000 L 23.000,17.000 L 23.000,1.000 L 16.000,5.000 L 8.000,1.000 L 1.000,5.000 Z"/>
