                        <!-- Capa 1/<Trazado> -->
                        <Path StrokeThickness="2.0" Stroke="#FF343434" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeLineJoin="Round" Data="F1 M 16.000,5.000 L 16.000,21.000"/>
                    </Canvas>
                </Canvas>
            </Grid>
            <Grid Width="42" HorizontalAlignment="Left" Margin="171,0,0,0" Grid.Column="1">
                <Canvas Width="18" Height="20.000">
                    <Canvas>
                        <!-- Capa 1/<Trazado> -->
                        <Path StrokeThickness="2.0" Stroke="#FF343434" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeLineJoin="Round" Data="F1 M 3.000,1.000 L 13.000,1.000 C 14.105,1.000 15.000,1.895 15.000,3.000 L 15.000,19.000 C 15.000,20.105 14.105,21.000 13.000,21.000 L 3.000,21.000 C 1.895,21.000 1.000,20.105 1.000,19.000 L 1.000,3.000 C 1.000,1.895 1.895,1.000 3.000,1.000 Z"/>
