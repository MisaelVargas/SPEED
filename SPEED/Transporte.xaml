        <Grid VerticalAlignment="Top" HorizontalAlignment="Left" Width="429" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="124*"/>
                <ColumnDefinition Width="305*"/>
            </Grid.ColumnDefinitions>
            <!-- <Rectangle HorizontalAlignment="Left" Height="43" VerticalAlignment="Center" Width="129" Fill="#FFFAFAFA" Margin="0,-0.5"/>-->
            <Grid Width="42" Height="42" HorizontalAlignment="Left" Margin="10,0,0,0">
                <Canvas Width="18.000" Height="20.000" Margin="0">
                    <Canvas>
                        <!-- Capa 1/<Trazado> -->
                        <Path StrokeThickness="2.0" Stroke="#FF343434" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeLineJoin="Round" Data="F1 M 1.000,1.000 L 16.000,1.000 L 16.000,14.000 L 1.000,14.000 L 1.000,1.000 Z"/>
                        <!-- Capa 1/<Trazado> -->
                        <Path StrokeThickness="2.0" Stroke="#FF343434" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeLineJoin="Round" Data="F1 M 16.000,6.000 L 20.000,6.000 L 23.000,9.000 L 23.000,14.000 L 16.000,14.000 L 16.000,6.000 Z"/>
                        <!-- Capa 1/<Trazado> -->
                        <Path StrokeThickness="2.0" Stroke="#ff000000" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeLineJoin="Round" Data="F1 M 5.500,14.000 C 6.881,14.000 8.000,15.119 8.000,16.500 C 8.000,17.881 6.881,19.000 5.500,19.000 C 4.119,19.000 3.000,17.881 3.000,16.500 C 3.000,15.119 4.119,14.000 5.500,14.000 Z"/>
